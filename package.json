{
  "name": "node-project-template",
  "version": "1.0.0",
  "description": "",
  "main": "dist/server.js",
  "author": "Ayodeji Abodunrin <https://ayodejiabodunrin.com/>",
  "keywords": [],
  "scripts": {
    "start:dev": "nodemon",
    "start": "node dist/server.js",
    "build": "tsc",
    "postbuild": "copyfiles -u 1 \"src/**/*.yaml\" dist",
    "prepare": "husky install",
    "test": "NODE_ENV=test jest --runInBand --passWithNoTests",
    "lint": "eslint --ignore-path .eslintignore --ext .ts .",
    "lint:fix": "eslint . --ext .ts --fix",
    "knex": "ts-node -r dotenv/config ./node_modules/knex/bin/cli.js",
    "db:migrate:make": "npm run knex -- migrate:make",
    "db:migrate": "npm run knex -- migrate:latest",
    "db:migrate:rollback": "npm run knex -- migrate:rollback",
    "db:seed": "npm run knex -- seed:run"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^27.4.1",
    "@types/node": "^22.3.0",
    "@types/stream-to-array": "^2.3.3",
    "@types/useragent": "^2.3.4",
    "@typescript-eslint/eslint-plugin": "^5.19.0",
    "@typescript-eslint/parser": "^5.19.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^26.1.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^5.2.0",
    "husky": "^7.0.4",
    "jest": "^27.5.1",
    "lint-staged": "^12.4.0",
    "nodemon": "^2.0.15",
    "prettier": "^2.6.2",
    "ts-jest": "^27.1.4",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.3"
  },
  "dependencies": {
    "@types/nodemailer": "^6.4.16",
    "@types/nodemailer-express-handlebars": "^4.0.5",
    "axios": "^0.27.2",
    "bcrypt": "^5.1.1",
    "copyfiles": "^2.4.1",
    "cors": "^2.8.5",
    "csv-writer": "^1.6.0",
    "date-fns": "^2.30.0",
    "dotenv": "^16.0.0",
    "event-dispatch": "~0.4.1",
    "express": "^4.19.2",
    "google-auth-library": "^9.4.1",
    "handlebars": "^4.7.8",
    "http-status": "^1.5.1",
    "install": "^0.13.0",
    "ioredis": "~5.0.4",
    "jsonwebtoken": "^8.5.1",
    "knex": "^2.4.0",
    "module-alias": "^2.2.2",
    "ms": "^2.1.3",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.9.15",
    "nodemailer-express-handlebars": "^7.0.0",
    "objection": "^3.0.1",
    "pg": "^8.7.3",
    "pino": "^7.10.0",
    "randomatic": "^3.1.1",
    "reflect-metadata": "^0.1.13",
    "slugify": "^1.6.6",
    "tsyringe": "^4.6.0",
    "useragent": "^2.3.0",
    "uuid": "^9.0.1",
    "validatorjs": "^3.22.1"
  },
  "_moduleAliases": {
    "@config": "dist/config",
    "@shared": "dist/shared",
    "@utils": "dist/shared/utils"
  },
  "lint-staged": {
    "**/*.{js, ts}": [
      "npm run lint -- --fix",
      "npx prettier --write ./src"
    ]
  }
}
